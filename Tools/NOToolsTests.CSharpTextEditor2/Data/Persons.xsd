<?xml version="1.0" encoding="utf-8"?>
<element name="Person">
  <complexType>
    <sequence>
      <element name="ID" type="System.Int32" readonly="true"/>
      <element name="Name" type="System.String" readonly="false"/>
      <element name="Company" type="System.String" readonly="false"/>
      <element name="City" type="System.String" readonly="false"/>
      <element name="PostalCode" type="System.String" readonly="false"/>
      <element name="Country" type="System.String" readonly="false"/>
      <element name="Phone" type="System.String" readonly="false"/>
    </sequence>
  </complexType>
</element>
